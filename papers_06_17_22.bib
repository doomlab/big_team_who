@article{buchanan_measuring_2021,
	title = {Measuring the {Semantic} {Priming} {Effect} {Across} {Many} {Languages}},
	url = {https://osf.io/q4fjy},
	abstract = {Semantic priming has been studied for nearly 50 years across various experimental manipulations and theoretical frameworks. These studies provide insight into the cognitive underpinnings of semantic representations in both healthy and clinical populations; however, they have suffered from several issues including generally low sample sizes and a lack of diversity in linguistic implementations. Here, we will test the size and the variability of the semantic priming effect across ten languages by creating a large database of semantic priming values, based on an adaptive sampling procedure. Differences in response latencies between related word-pair conditions and unrelated word-pair conditions (i.e., difference score confidence interval is greater than zero) will allow quantifying evidence for semantic priming, whereas improvements in model fit with the addition of a random intercept for language will provide support for variability in semantic priming across languages.},
	urldate = {2022-06-17},
	author = {Buchanan, Erin Michelle and Cuccolo, Kelly and Coles, Nicholas Alvaro and Heyman, Tom and Iyer, Aishwarya and Lewis, Neil Anthony and Peters, Kim Olivia and van Berkel, Niels and van 't Veer, Anna Elisabeth and Taylor, Jack Edward and Montefinese, Maria and Valentine, Kathrene D and Maxwell, Nicholas P. and Türkan, Nilay and Williams, Glenn Patrick and Oliveros-Chacana, Juan C. and Röer, Jan Philipp and Fini, Chiara and Acar, Oguz Ali and McFall, Joseph Patrick and Pronizius, Ekaterina and Suchow, Jordan W and Batalha, Luisa and Özdoğru, Asil Ali and Godbersen, Hendrik and Butt, Muhammad Mussaffa and Buczny, Jacek and Jaeger, Bastian and Baker, Bradley James and Grim, Philip A. and Alsuhaibani, Zainab and Martínez, Martín and Protzko, John and Lynott, Dermot and Korbmacher, Max and Peker, Mhmet and Dixson, Barnaby and Elsherif, Mahmoud Medhat and Neta, Maital and Azevedo, Flavio and Ferreira, Paulo RS and Sigfrids, Fredrik and Lima, Tiago Jessé Souza and Geiger, Sandra Jeanette and Thapar, Anjali and Perea, Manuel and Szekely-Copîndean, Raluca Diana and Evans, Thomas Rhys and Verheyen, Steven and Moreau, David and Tran, Ulrich S. and El-Dakhs, Dina Abdel Salam and Ndukaihe, Izuchukwu Lawrence Gabriel and Pavlović, Tijana Vesić and Burin, Di and Arriaga, Patrícia and Kasanov, Dauren and Keech, Jacob J and Fernández-López, María and Stewart, Suzanne and Vaidis, David C. and Besson, Théo and Batres, Carlota and Vaughn, Leigh Ann and Senderecka, Magdalena and Mazzuca, Claudia and Micheli, Leticia and Vasilev, Martin R. and Schmidt, Kathleen and Brick, Cameron and Schivinski, Bruno and Ruiz-Fernandez, Susana and Ilczuk, Ewa and Levitan, Carmel and Higgins, Emily and Pfuhl, Gerit and Lu, Jackson G. and Sirota, Miroslav and Pavlović, Zoran and Ambrosini, Ettore and Bohm, Nienke and Karaaslan, Aslan and Papadatou-Pastou, Marietta and Oner, Sezin and Baskin, Ernest and Mulgrew, Kate E. and Ulloa Fulgeri, Jose Luis and Szumowska, Ewa and Garrido-Vásquez, Patricia a