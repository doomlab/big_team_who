@article{chen_investigating_2018,
	title = {Investigating {Object} {Orientation} {Effects} {Across} 14 {Languages}},
	url = {https://osf.io/t2pjv},
	abstract = {Mental simulation theories of language comprehension propose that people automatically create mental representations of real objects. Evidence from sentence-picture verification tasks has shown that people mentally represent various visual properties such as shape, color, and size. However, the evidence for mental simulations of object orientation is limited. We report a study that investigates the match advantage of object orientation across speakers of different languages. This multi-laboratory project aims to achieve two objectives. First, we examine the replicability of the match advantage of object orientation across multiple languages and laboratories. Second, we will use a mental rotation task to measure participants’ mental imagery after the sentence-picture verification task. The relationship between the participants’ performance of the two tasks will provide a cross-linguistic examination of perceptual simulation processes. With the (broad) evaluation of individual mental imagery ability and potential linguistic moderators, we expect a robust estimation of match advantage of object orientation.},
	urldate = {2022-02-21},
	author = {Chen, Sau-Chin and Buchanan, Erin and Kekecs, Zoltan and Miller, Jeremy and Szabelska, Anna and Aczel, Balazs and Bernabeu, Pablo and Forscher, Patrick and Szuts, Attila and Vally, Zahir and Al-Hoorie, Ali and Helmy, Mai and de Silva, Caio and da Silva, Luana and de Moraes, Yago and Hsu, Rafael and Mafra, Anthonieta and Valentova, Jaroslava and Varella, Marco and Dixon, Barnaby and Peters, Kim and Steffens, Nik and Ghaesmi, Omid and Roberts, Andrew and Ross, Robert and Stephan, Ian and Milyavskaya, Marina and Wang, Kelly and Werner, Kaitlyn and Holford, Dawn and Sirota, Miroslav and Evans, Thomas and Lynott, Dermot and Lane, Bethany and Riis, Danny and Williams, Glenn and Tan, Chrystalle and Foo, Alicia and Janssen, Steve and Arinze, Nwadiogo and Gabriel, Izuchukwu and Moreau, David and Jurosic, Brianna and Leach, Brynna and Lewis, Savannah and Mallik, Peter and Schmidt, Kathleen and Chopik, William and Vaughn, Leigh and Li, Manyu and Levitan, Carmel and Storage, Daniel and Batres, Carlota and Enachescu, Janina and Olsen, Jerome and Voracek, Martin and Lamm, Claus and Pronizius, Ekaterina and Ripp, Tilli and Röer, Jan and Schnepper, Roxane and Papadatou-Pastou, Marietta and Mokady, Aviv and Reggev, Niv and Chandel, Priyanka and Kujur, Pratibha and Pande, Babita and Parganiha, Arti and Parveen, Noorshama and Pradhan, Sraddha and Singh, Margaret and Korbmacher, Max and Kunst, Jonas and Tamnes, Christian and Woelfert, Frederike and Klevjer, Kristoffer and Martiny, Sarah and Pfuhl, Gerit and Adamus, Sylwia and Barzykowski, Krystian and Filip, Katarzyna and Arriaga, Patrícia and Gvozdenović, Vasilije and Kovic, Vanja and Gan, Tao-tao and Hu, Chuan-Peng and Liu, Qing-Lan and Chen, Zhong and Gao, Fei and Li, Lisa and Bavolár, Jozef and Hricová, Monika and Kacmár, Pavol and Adamkovic, Matúš and Babincák, Peter and Baník, Gabriel and Ropovik, Ivan and Ricaurte, Danilo and Solas, Sara and Manley, Harry and Suavansri, Panita and Kung, Chun-Chia and Çoktok, Belemir a