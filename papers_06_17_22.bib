@article{legate_global_2022,
	title = {A {Global} {Experiment} on {Motivating} {Social} {Distancing} during the {COVID}-19 {Pandemic}},
	url = {https://psyarxiv.com/n3dyf/},
	abstract = {Finding communication strategies that effectively motivate social distancing continues to be a global public health priority during the COVID-19 pandemic. This cross-country, preregistered experiment (n = 25,718 from 89 countries) tested hypotheses concerning generalizable positive and negative outcomes of social distancing messages that promoted personal agency and reflective choices (i.e., an autonomy-supportive message) or were restrictive and shaming (i.e. a controlling message) compared to no message at all. Results partially supported experimental hypotheses in that the controlling message increased controlled motivation (a poorly-internalized form of motivation relying on shame, guilt, and fear of social consequences) relative to no message. On the other hand, the autonomy-supportive message reduced feelings of defiance relative to the controlling message. Unexpectedly, messages did not influence autonomous motivation (a highly-internalized form of motivation relying on one’s core values) or behavioral intentions. Results supported hypothesized associations between people’s existing autonomous and controlled motivations and self-reported behavioral intentions to engage in social distancing: Controlled motivation was associated with more defiance and less long-term behavioral intentions to engage in social distancing, whereas autonomous motivation was associated with less defiance and more short- and long-term intentions to social distance. Overall, this work highlights the potential harm of using shaming and pressuring language in public health communication, with implications for the current and future global health challenges.},
	language = {en-us},
	urldate = {2022-02-23},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Legate, Nicole and Nguyen, Thuy-vy and Weinstein, Netta and Moller, Arlen and Legault, Lisa and Maniaci, Michael R. and Ebersole, Charles R. and Adamkovic, Matus and Adetula, Dr Gabriel Agboola and Agesin, Bamikole Bamikole and Ahlgren, Lina and Akkas, Handan and Almeida, Inês and Anjum, Gulnaz and Antoniadi, Maria and Arinze, Azuka Ikechukwu and Arvanitis, Alexios and Rana, Kafeel and Badalyan, Varduhi and Becker, Maja and Bernardo, Olivia and Bozdoc, Andreea Ioana and Campos, Olatz and Desai, Kermeka and Dursun, Pinar and Eudave, Luis and Filip, Katarzyna and Barría, Mauricio F. Espinoza and Garcia, Elkin Oswaldo Luis and Gialitaki, Zoi and Nardelli, Giusy and Štrukelj, Eva and Ozery, Daphna Hausman and Jackson, Emily A. and Jimenez-Leal, William and Krizanic, Valerija and Jurković, Anita Penić and Wilson, John Paul and Alarcon, Monica and udeh.oluchukwu@gmail.com and Betlehem, Ruben and Kushnir, Elizaveta and Bravo, Olalla Niño and Nauman, Rafia and Oosterlinck, Marlies and Pantazi, Myrto and Pilecka, Natalia and Szabelska, Anna and Steenkiste, I. M. M. van and Marcu, Gabriela and Roczniewska, Marta and Reyna, Cecilia and Kassianos, Angelos and Pöntinen, Sara and Ogbonnaya, Chisom and Ndukaihe, Izuchukwu and Dalgar, Ilker and Ozdogru, Asil and Macapagal, Paulo Manuel L. and Lewis, Savannah and Orta, Irem Metin and Francesco, Foroni and Willis, Megan and Santos, Anabela Caetano and Mokady, Aviv and Reggev, Niv and Vasilev, Martin Rachev and Nock, Nora L. and Parzuchowski, Michal and Vranka, Marek A. and Kohlova, Marketa Braun and Ropovik, Ivan and Harutyunyan, Mikayel and Wang, Chunhui and Yao, Xiaohui and Manunta, Efisio and Kaminski, Gwenael and Findor, Andrej and Landry, Anais Thibault and Lewis, David M. G. and Vásquez, Julio E. Cruz and Ortiz, Manuel S. and Pronizius, Ekaterina and Voracek, Martin and Lamm, Claus and Grinberg, Maurice and Li, Ranran and Valentova, Jaroslava Varella and Mioni, Giovanna and Azouaghe, Soufian and Aruta, John Jamir Ben