@techreport{aguenounon_evolution_2021,
	type = {preprint},
	title = {The evolution of primate short-term memory},
	url = {https://osf.io/5etnf},
	abstract = {Short-term memory is implicated in a range of cognitive abilities and is critical for understanding primate cognitive evolution. To investigate the effects of phylogeny, ecology and sociality on short-term memory ability, we tested 421 non-human primates across 41 species in a pre-registered, experimental delayed-response task. Our results confirm previous findings that longer delays decrease memory performance across species and taxa. Our analyses demonstrate a considerable contribution of phylogeny over ecological and social factors on the distribution of short-term memory performance in primates; closely related species had more similar short-term memory abilities. However, interdependencies between phylogeny and socioecology of a given species present an obstacle to disentangling the effects of each of these factors on the evolution of short-term memory capacity. The dataset corresponding to the study is freely accessible and constitutes an important resource for studying the evolution of primate cognition.},
	urldate = {2022-01-07},
	institution = {PsyArXiv},
	author = {Aguenounon, Géraud and Allritz, Matthias and Altschul, Drew and Ballesta, Sébastien and Beaud, Alice and Bohn, Manuel and Bornbusch, Sally and Brandão, Angela and Brooks, James and Bustamante, Léa and Call, Josep and Canteloup, Charlotte and Cao, Chuangshi and Caspar, Kai and de Sousa, Alexandra and DeTroy, Sarah and Duguid, Shona and Eppley, Timothy and Fichtel, Claudia and Fischer, Julia and Gong, Chi and Grange, James and Grebe, Nicholas and Hanus, Daniel and Haun, Daniel and Haux, Lou and Héjja-Brichard, Yseult and Helman, Annabella and Hernadi, Istvan and Hernandez-Aguilar, R. and Herrmann, Esther and Hopper, Lydia and Howard, Lauren and Huang, Lei and Huskisson, Sarah and Jacobs, Ivo and Jin, Zhiyong and Joly, Marine and Kano, Fumihiro and Keupp, Stefanie and Kiefer, Evelin and Knakker, Balázs and Kóczán, Katalin and Kraus, Larissa and Kwok, Sze and Lefrançois, Marie and Lewis, Laura and Liu, Siyi and Llorente, Miquel and Lonsdorf, Elizabeth and Loyant, Louise and Majecka, Katarzyna and Maurits, Luke and Meunier, Hélène and Mobili, Flávia and Morino, Luca and Motes-Rodrigo, Alba and Nijman, Vincent and Ihomi, Caroline and Persson, Tomas and Pietraszewski, Dariusz and Parrish, Juan and Roig, Anthony and Sánchez-Amaro, Alejandro and Sato, Yutaro and Sauciuc, Gabriela-Alina and Schrock, Allie and Seed, Amanda and Shearer, Caroline and Šlipogor, Vedrana and Su, Yanjie and Tan, Jingzhi and Taylor, Derry and Troisi, Camille and Völter, Christoph and Warren, Elizabeth and Watzek, Julia and Zablocki-Thomas, Pauline},
