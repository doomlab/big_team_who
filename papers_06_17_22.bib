@article{coles_multi-lab_2022,
	title = {A {Multi}-{Lab} {Test} of the {Facial} {Feedback} {Hypothesis} by {The} {Many} {Smiles} {Collaboration}},
	url = {https://osf.io/cvpuw},
	abstract = {Following theories of emotional embodiment, the facial feedback hypothesis suggests that an individual’s subjective experience of emotion is influenced by their facial expressions. Evidence for this hypothesis, however, has been mixed. We formed a global adversarial collaboration designed to specify and test the conditions that should most reliably produce facial feedback effects. Data from 3,878 hypothesis-unaware participants from 19 countries indicated that a facial mimicry and voluntary facial action task could both amplify and initiate feelings of happiness. Evidence, however, was less conclusive when unobtrusively manipulating facial feedback via a pen-in-mouth task. When present, the effects of facial feedback on happiness were similar in size to the effect of mildly pleasant images.},
	urldate = {2022-06-17},
	journal = {Nature Human Behaviour},
	author = {Coles, Nicholas Alvaro and March, David Scott and Marmolejo-Ramos, Fernando and Larsen, Jeff T. and Arinze, Nwadiogo Chisom and Ndukaihe, Izuchukwu Lawrence Gabriel and Willis, Megan and Foroni, Francesco and Reggev, Niv and Mokady, Aviv and Forscher, Patrick S. and Hunter, John and Kaminski, Gwenaël and Yuvruk, Elif and Kapucu, Aycan and Nagy, Tamas and Hajdu, Nandor and Tejada, Julian and Freitag, Raquel Meister Ko. and Zambrano, Danilo and Som, Bidisha and Aczel, Balazs and Barzykowski, Krystian and Adamus, Sylwia and Filip, Katarzyna and Yamada, Yuki and Ikeda, Ayumi and Eaves, Daniel and Levitan, Carmel and Leiweke, Sydney and Parzuchowski, Michal and Butcher, Natalie and Pfuhl, Gerit and Basnight-Brown, Dana and Hinojosa, José Antonio and Montoro, Pedro R. and Delgado, Grey Javela and Vezirian, Kevin and IJzerman, Hans and Trujillo, Natalia and Pressman, Sarah and Gygax, Pascal and Özdoğru, Asil Ali and Ruiz-Fernandez, Susana and Ellsworth, Phoebe C. and Gaertner, Lowell and Strack, Fritz and Liuzza, Marco Tullio and Marozzi, Marco},
	year = {2022},
	doi = {10.31234/osf.io/cvpuw},
