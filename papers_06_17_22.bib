	keywords = {cooperation, economic games, decision making, replication, social heuristic hypothesis, social psychology},
	pages = {527--542},
	file = {SAGE PDF Full Text:/Users/erinbuchanan/Zotero/storage/KNISVWY9/Bouwmeester et al. - 2017 - Registered Replication Report Rand, Greene, and N.pdf:application/pdf},
}

