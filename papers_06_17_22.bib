@article{forscher_stereotype_2019,
	title = {Stereotype {Threat} in {Black} {College} {Students} {Across} {Many} {Operationalizations}},
	url = {https://osf.io/6hju9},
	abstract = {According to stereotype threat theory, the possibility of confirming a negative group stereotype evokes feelings of threat, leading people to underperform in domains where they are stereotyped as lacking ability. This theory has immense theoretical and practical implications. However, many studies supporting it include small samples and varying operational definitions of “stereotype threat”. We address the first challenge by leveraging a network of psychology labs to recruit a large Black student sample (Nanticipated = 2700) from multiple US sites (Nanticipated = 27). We address the second challenge by identifying three threat-increasing and three threat-decreasing procedures that could plausibly affect performance and use an adaptive Bayesian design to determine which operationalization yields the strongest evidence for underperformance. This project should advance our knowledge of a scientifically and socially important topic: the conditions under which stereotype threat affects performance among current Black students in the United States.},
	urldate = {2022-02-21},
	author = {Forscher, Patrick S. and Taylor, Valerie Jones and Cavagnaro, Daniel and Lewis, Neil Anthony and Buchanan, Erin Michelle and Moshontz, Hannah and Mark, Aimee Y. and Appleby, Sara and Batres, Carlota and Bennett-Day, Brooke and Chopik, William J. and Damian, Rodica I. and Ellis, Claire E. and Faas, Caitlin and Gaither, Sarah and Green, Dorainne and Hall, Braeden Forrester and Hinojosa, Bianca Marie and Howell, Jennifer Lee and Johnson, David Charles and Kung, Franki Y. H. and Laird, Angie and Levitan, Carmel and Li, Manyu and Maddox, Keith Brian and Murphy, Mary C and Musser, Erica D. and Pankey, Brianna and Parker, Laura Ruth and Perry, Sylvia and Remedios, Jessica D. and Schmidt, Kathleen and Serrano, Surizaday and Steltenpohl, Crystal Nicole and Storage, Daniel and Straka, Brenda and Urry, Heather L. and Wasmuth, Samuel and Westgate, Erin Corwin and Wilson, John Paul and Wynn, Shelby and Zimmerman, David Michael and Peters, Kim Olivia and Chartier, Christopher R.},
